<h1>New Users</h1>
 
<%= form_with model: @user, local: true do |form| %>

    <p>
        <%= form.label :name %><br>
        <%= form.text_field :name %>
    </p>
    
    <p>
        <%= form.label :email %><br>
        <%= form.text_area :email %>
    </p>
    
    <p>
        <%= form.submit %>
    </p>
    
    <%= link_to 'Back', users_path %>

<%end%>